sc_agent_for_searching_carries_upper_port
=> nrel_main_idtf:
	[sc-агент поиска протоколов в осях OSI]
    (*
        <- lang_ru;;
    *);
	[sc-agent for searching carries in OSI layers]
    (* 
        <- lang_en;;
    *);
<- abstract_sc_agent;
=> nrel_primary_initiation_condition: 
	(sc_event_after_generate_outgoing_arc => action_initiated);
=> nrel_initiation_condition_and_result: 
	(..sc_agent_for_searching_carries_upper_port_condition => ..sc_agent_for_searching_carries_upper_port_result);
=> nrel_sc_agent_action_class:
	action_search_carries_upper_port;
<= nrel_sc_agent_key_sc_elements: 
{
	action_initiated;
	action_search_carries_upper_port
};
=> nrel_inclusion: 
	.platform_independent_implementation_of_sc_agent_for_searching_carries_upper_port
	(*
		<- platform_independent_abstract_sc_agent;;
		<= nrel_sc_agent_program: 
		{
			agent_program_for_searching_carries_upper_port;
			program_for_searching_carries_upper_port
		};;

		-> scp_agent_for_searching_carries_upper_port
		(*
			<- active_sc_agent;;
		*);;  
	*);;

..sc_agent_for_searching_carries_upper_port_condition
= [*
	action_search_carries_upper_port _-> .._action;;
	action_initiated _-> .._action;;
	.._action _-> rrel_1:: .._layer;;
*];;

..sc_agent_for_searching_carries_upper_port_result
= [*
	concept_network_protocol _-> .._carrie;;
*];;
